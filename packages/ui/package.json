{
	"name": "@chiron/ui",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"main": "src/index.ts",
	"types": "src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts",
			"default": "./src/index.ts"
		}
	},
	"scripts": {
		"build": "tsup src/index.ts --dts --format esm,cjs",
		"dev": "tsup src/index.ts --watch",
		"lint": "biome check ./src",
		"format": "biome format ./src --write"
	},
	"dependencies": {
		"@chiron/design-tokens": "workspace:*",
		"framer-motion": "12.23.22"
	},
	"peerDependencies": {
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	},
	"devDependencies": {
		"@types/react": "19.2.2",
		"@types/react-dom": "19.2.2",
		"tsup": "8.5.0",
		"typescript": "5.9.3"
	}
}
